$rundata.setLayoutEnabled(false)
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="/css/index.css"	rel="stylesheet" />
<link href="/css/layout-v2.css"	rel="stylesheet" />
<script src="/js/jquery-1.8.1.js" type="text/javascript"></script>
<script src="/js/index.js" type="text/javascript"></script>
<title>浙江省邮政-登录</title>
</head>
<body>
	<div class="mod-main">
		<div class="obj-header">
			<div class="bar">
				<ul>
					<li>欢迎 <span id="userName">您 &nbsp;$!{username}</span>&nbsp;<a href="#">退出</a></li>
					<li class="faverate">我要收藏</li>
					<li class="rss">RSS</li>
				</ul>
			</div>
			<div class="menu">
				<ul class="bundle">
					<li class="init selected">首页</li>
					<li class="init">档案任务</li>
					<li class="init">档案查询</li>
					<li class="init"><a href="/youdianbao.html" target="_blank"><font color=white>邮电报</font></a></li>
					<li class="init">史料公布</li>
					<li class="init">馆藏指南</li>
					<li class="init">使用帮助</li>
				</ul>
			</div> 
	    </div>
   <div class="obj-content">
   		<form name="form1" method="post" action="/home/login.htm">
   		<input type="hidden" name="action" value="loginAction"/>
		<input type="hidden" name="event_submit_do_login" value="true" />
   		<div class="c_login">
   		     <div class="errorMsg"><font color="red">$!{errorMsg}</font></div>
             <div class="c_user"> 用户名：<input type="text" id="username" class="search-input" value="$!{username}" name="username" /></div>
             <div class="c_pass"> 密&nbsp;&nbsp;&nbsp;码：<input type="password" id="password" class="search-input" value="" name="password" /></div>
             <div class="c_btn"> 
              <input type="submit" id="login-btn" value="登录" class="search-btn"/>
              &nbsp; &nbsp; &nbsp;
              <input type="reset" id="login-btn" value=" 重置" class="search-btn"/>
             </div>
	    </div>
	    </form>
		<div class="c_bottom">
		    copy right 2012-2013 浙江省邮政公司
		</div>
	</div>
</div>
</body>
</html>
